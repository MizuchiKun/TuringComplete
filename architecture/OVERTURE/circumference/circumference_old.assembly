const pi 3
const 2pi 2*pi

# reg4: radius
# reg5: 

# Loop start.
label circ_loop
	# Save radius in reg4.
	in_to_reg4

	# Calculate circumference by 
	# repeated addition (x 6 (2pi)).
	reg4_to_reg1
	reg4_to_reg2
	add  # 2r.
	reg3_to_reg1
	add  # 3r.
	reg3_to_reg1
	add  # 4r.
	reg3_to_reg1
	add  # 5r.
	reg3_to_reg1
	add  # 6r.
	
	# Output result.
	reg3_to_out

# Jump to loop start.
circ_loop
jump
