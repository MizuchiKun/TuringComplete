# Initialise ram_addr.
movi 0x0 to ram_addr

label save_loop
# Save input to ram.
mov in to ram
# Increment ram_addr.
add|i2 ram_addr 1 ram_addr
# Jump to start of loop.
jl|i2 ram_addr 32 save_loop

# Reset ram_addr.
movi 0x0 to ram_addr

label load_loop
# Load from ram to out.
mov ram to out
# Increment ram_addr.
add|i2 ram_addr 1 ram_addr
# Jump to start of loop.
jl|i2 ram_addr 32 load_loop
